{
  "name": "Beatport import artists to follow",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "",
  "homepage_url": "http://localhost",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Beatport import artists to follow",
    "default_popup": "src/popup/popup.html"
  },
  "permissions": [
    "https://*.beatport.com/*",
    "webRequest",
    "tabs"
  ]
}